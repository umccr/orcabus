{
  "name": "orcabus",
  "version": "0.1.0",
  "author": {
    "name": "UMCCR"
  },
  "repository": {
    "url": "https://github.com/umccr/orcabus.git",
    "type": "git"
  },
  "license": "MIT",
  "bin": "bin/pipeline.js",
  "scripts": {
    "test": "tsc && jest",
    "cdk": "cdk",
    "orcabus": "cdk --app 'npx ts-node --prefer-ts-exts bin/orcabus.ts'",
    "build": "cdk synth -q",
    "watch": "tsc -w",
    "clean": "tsc --build --clean",
    "audit": "yarn npm audit",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "black": "black -t py310 --check ./lib --exclude skel --exclude (**/.venv)",
    "black-fix": "black -t py310 ./lib --exclude skel --exclude (**/.venv)"
  },
  "dependencies": {
    "@aws-cdk/aws-apigatewayv2-alpha": "^2.110.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-authorizers-alpha": "^2.110.0-alpha.0",
    "@aws-cdk/aws-apigatewayv2-integrations-alpha": "^2.110.0-alpha.0",
    "@aws-cdk/aws-lambda-python-alpha": "2.110.0-alpha.0",
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0",
    "dotenv": "^16.3.1",
    "rust.aws-cdk-lambda": "^1.2.1",

    "constructs": "^10.2.69",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "aws-cdk": "2.110.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.6.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "semver": "7.5.2"
  },
  "packageManager": "yarn@3.5.1"
}
