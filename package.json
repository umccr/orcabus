{
  "name": "orcabus",
  "version": "0.1.0",
  "author": {
    "name": "UMCCR"
  },
  "repository": {
    "url": "https://github.com/umccr/orcabus.git",
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "test": "tsc && jest",
    "build": "cdk synth -q",
    "watch": "tsc -w",
    "clean": "tsc --build --clean",
    "audit": "yarn npm audit",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "cdk-stateless": "cdk --app 'yarn run -B ts-node --prefer-ts-exts bin/statelessPipeline.ts'",
    "cdk-stateful": "cdk --app 'yarn run -B ts-node --prefer-ts-exts bin/statefulPipeline.ts'"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.152.0-alpha.0",
    "@aws-cdk/aws-pipes-alpha": "^2.152.0-alpha.0",
    "@aws-cdk/aws-pipes-sources-alpha": "^2.152.0-alpha.0",
    "aws-cdk-lib": "^2.152.0",
    "cargo-lambda-cdk": "^0.0.22",
    "cdk-nag": "^2.28.181",
    "constructs": "^10.3.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21",
    "sqs-dlq-monitoring": "^1.2.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.3.0",
    "aws-cdk": "^2.152.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  },
  "packageManager": "yarn@4.4.0"
}
