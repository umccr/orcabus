{
  "name": "orcabus",
  "version": "0.1.0",
  "author": {
    "name": "UMCCR"
  },
  "repository": {
    "url": "https://github.com/umccr/orcabus.git",
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "test": "tsc && jest",
    "build": "cdk synth -q",
    "watch": "tsc -w",
    "clean": "tsc --build --clean",
    "audit": "yarn npm audit",
    "lint": "eslint .",
    "lint-fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "cdk-stateless-pipeline": "cdk --app 'yarn run -B ts-node --prefer-ts-exts bin/stateless-pipeline.ts'",
    "cdk-stateful-pipeline": "cdk --app 'yarn run -B ts-node --prefer-ts-exts bin/stateful-pipeline.ts'",
    "cdk-orcabus": "cdk --app 'yarn run -B ts-node --prefer-ts-exts bin/orcabus.ts'"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-python-alpha": "2.126.0-alpha.0",
    "aws-cdk-lib": "^2.126.0",
    "cargo-lambda-cdk": "^0.0.19",
    "cdk-nag": "^2.28.27",
    "constructs": "^10.2.69",
    "dotenv": "^16.3.1",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.4.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "aws-cdk": "^2.126.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.6.1",
    "jest-junit": "^16.0.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "resolutions": {
    "semver": "7.5.2"
  },
  "packageManager": "yarn@3.5.1"
}
